{
    "name": "akbarhossainr/laravel-erd-generator",
    "description": "Laravel package to generate ERDs from Eloquent models (Mermaid.js, PlantUML, Graphviz)",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Akbarhossainr\\ErdGenerator\\": "src/"
        }
    },
    "require": {
        "php": ">=8.0",
        "illuminate/support": ">=8.0",
        "illuminate/console": ">=8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Akbarhossainr\\ErdGenerator\\ErdGeneratorServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^12.1",
        "orchestra/testbench": "^10.3",
        "mockery/mockery": "^1.6"
    },
    "scripts": {
        "test": "phpunit --configuration=phpunit.xml --testdox"
    }
}
